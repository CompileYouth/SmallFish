{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "comma-dangle": [
        "off"
      ],
      "no-use-before-define": [
        "off"
      ],
      "no-underscore-dangle": [
        "off"
      ]
    }
}
